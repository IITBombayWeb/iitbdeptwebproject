{
    "name": "iitbombayweb/iitbdeptwebproject",
    "description": "IIT Bombay Department Website Distribution Project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3.0",
        "drupal/core-recommended": "~9.1.0",
        "drupal/core-composer-scaffold": "~9.1",
        "szeidler/composer-patches-cli": "~1.0.0",
        "oomphinc/composer-installers-extender": "~2.0",
        "wikimedia/composer-merge-plugin": "~2.0",
        "drupal/console": "~1.0",
        "drush/drush": "^8.0",
        "solarium/solarium": "~4.2",
        "iitbombayweb/iitbdeptweb": "dev-dev#9cffdf946b9f2a1eae54d71b0c1f0ed03fc66175",
        "iitbombayweb/iitbcommon": "dev-dev#ed89d747b1fd0bc586547cae677c3080ba80b1c6",
        "iitbombayweb/iitbextra": "dev-dev#820fc5c7d505b91de05e1ee66c9e19e96a6d1425",
        "iitbombayweb/acadcommon": "dev-dev#85b4e2353164fb8dac6c937b7cfae1b56fa74ce6"
    },
    "conflict": {
        "symfony/http-foundation": "3.4.24"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "acadmixdist": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbdeptweb",
            "no-api": true
        },
        "iitbcommonmodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbcommon",
            "no-api": true
        },
        "iitbextramodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbextra",
            "no-api": true
        },
        "acadcommonmodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/acadcommon",
            "no-api": true
        }
    },
    "config": {
        "process-timeout": 0
    },
    "extra": {
        "merge-plugin": {
            "include": ["docroot/profiles/contrib/iitbdeptweb/composer.json"],
            "recurse": true,
            "replace": false,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "patch/{$name}": ["type:drupal-patch"]
        },
        "enable-patching": true,
        "patches-file": "composer.patches.json"
    },
    "scripts": {
        "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-update-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    }
}
