{
    "name": "iitbombayweb/iitbdeptwebproject",
    "description": "IIT Bombay Department Website Distribution Project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3.0",
        "drupal/core-recommended": "~9.3.0",
        "drupal/core-composer-scaffold": "~9.3",
        "szeidler/composer-patches-cli": "~1.0.0",
        "oomphinc/composer-installers-extender": "~2.0",
        "wikimedia/composer-merge-plugin": "~2.0",
        "drupal/console": "~1.0",
        "drush/drush": "^8.0",
        "solarium/solarium": "~6.1",
        "iitbombayweb/iitbdeptweb": "dev-dev#45fbdda602f13d968b302abdc4b95b6356c69a77",
        "iitbombayweb/iitbcommon": "dev-dev#0754de4ba634f7730b3088a548ef97b845a40b4f",
        "iitbombayweb/iitbextra": "dev-dev#7c4e2ba4a827d4e93dbc7c33cde51f70f2106370",
        "iitbombayweb/acadcommon": "dev-dev#599d2be6525f2c5b6719cbcdd293787091bacf54"
    },
    "conflict": {
        "symfony/http-foundation": "3.4.24"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "acadmixdist": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbdeptweb",
            "no-api": true
        },
        "iitbcommonmodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbcommon",
            "no-api": true
        },
        "iitbextramodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/iitbextra",
            "no-api": true
        },
        "acadcommonmodules": {
            "type": "github",
            "url": "https://github.com/iitbombayweb/acadcommon",
            "no-api": true
        },
        "groupmenu": {
            "type": "package",
            "package": {
                "name": "drupal_git/groupmenu",
                "type": "drupal-module",
                "version": "1.0-beta2",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/groupmenu.git",
                    "reference": "tags/8.x-1.0-beta2"
                }
            }
        }
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "szeidler/composer-patches-cli": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "merge-plugin": {
            "include": ["docroot/profiles/contrib/iitbdeptweb/composer.json"],
            "recurse": true,
            "replace": false,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "patch/{$name}": ["type:drupal-patch"]
        },
        "enable-patching": true,
        "patches-file": "composer.patches.json"
    },
    "scripts": {
        "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-update-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    }
}
